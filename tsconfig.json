{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "typeRoots": ["./types", "./node_modules/@types"],
    "paths": {
      "@/*": [
        "./*"
      ],
      "@jest/globals": [
        "./types/jest-globals"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "global.d.ts",
    "nativewind-env.d.ts"
  ]
}